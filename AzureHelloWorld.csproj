    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageIdPrefix>CustomPrefix.</PackageIdPrefix>  <!-- Add this line -->
    <PackageId>$(PackageIdPrefix)AzureHelloWorld</PackageId>  <!-- Uses the prefix -->
  </PropertyGroup>
</Project>
